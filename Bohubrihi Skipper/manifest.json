 {
     "name": "Bohubrihi Skipper",
     "description": "Skip Bohubrihi Lessons",
     "version": "1.0.0",
     "manifest_version": 2,
     "background": {
         "scripts": ["./script/background.js"],
         "persistent": false
     },
     "permissions": [
         "tabs",
         "https://www.bohubrihi.com/*"
     ],
     "icons": {
         "16": "./icon/icon16.png",
         "32": "./icon/icon32.png",
         "48": "./icon/icon48.png",
         "128": "./icon/icon128.png"
     },
     "browser_action": {
         "default_popup": "./html/popup.html"
     }
 }
