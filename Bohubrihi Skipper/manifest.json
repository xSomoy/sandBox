 {
     "name": "Bohubrihi Skipper",
     "description": "Skip Bohubrihi Lessons",
     "version": "1.0",
     "manifest_version": 2,
     "background": {
         "scripts": ["bg.js"]
     },
     "permissions": [
         "tabs",
         "https://www.bohubrihi.com/*"
     ],
     "browser_action": {
         "default_popup": "popup.html",
         "default_icon": "bls.png"
     }
 }
